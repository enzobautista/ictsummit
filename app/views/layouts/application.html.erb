      <ul class="nav navbar-nav">
        <li class="navlink">
        <%= link_to('ABOUT THE SUMMIT', root_path, data: { no_turbolink: true }) %></li>
        <li class="navlink"><a href="#">CIO FORUM MEMBERS</a></li>
        <li class="navlink"><a href="#">PROGRAM</a></li>
        <li class="dropdown navlink">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">SPONSORS <span class="caret"></span></a>
          <ul class="dropdown-menu">
            <li><a href="/sponsors">PLATINUM</a></li>
            <li><a href="#">GOLD</a></li>
            <li><a href="#">SILVER</a></li>
            <li><a href="#">SESSION</a></li>
