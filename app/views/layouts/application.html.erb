            <li role="separator" class="divider"></li>
            <li><a href="#">Separated link</a></li>
          </ul>
        </li>
        <li class="navlink"><a href="#">VENUE</a></li>
      </ul>

      
    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</nav>

<%= yield %>

<div id="footer-top">
<center>
<span id="register">REGISTER</span>
</center>
</div>
<div id="particles-js">
  <div class="row">
    <div class="col-md-4">
      <span id="cright">Ayala ICT Summit Â© 2016</span>
    </div>
    <div class="col-md-4">
      <center>
      <%=image_tag ('/assets/email.png'),:style=>"height:44px; margin-top:-12px;"%>
      </center>
    </div>
    <div class="col-md-4">
      <span id="developedby"><a id="dblink"href="https://github.com/enzobautista"><span id="codetext">< ></span> by Enzo Bautista</a></span>
    </div>
  </div>
  <center>
  <br>
  </center>
</div>
<!-- scripts -->
<script src="assets/particles.js"></script>
<script src="assets/app.js"></script>
  <script src="/assets/wow.min.js"></script>
  <script>
  new WOW().init();
  </script> 
	<script type="text/javascript">
		function checkScroll(){
		    var startY = $('.navbar').height() * 1.2; //adjusts where transition starts

		    if($(window).scrollTop() > startY){
		        $('.navbar').addClass("scrolled");
		    }else{
		        $('.navbar').removeClass("scrolled");
		    }
		}

		if($('.navbar').length > 0){
		    $(window).on("scroll load resize", function(){
		        checkScroll();
		    });
		}
		
	</script>
</body>
</html>
